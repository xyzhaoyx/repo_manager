[project]
	description = Manages all repos, prepares environment, runs it in the background, and shows status/logs
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/memento1
[access "refs/for/refs/*"]
	read = group user/memento1
	push = group user/memento1
	label-Code-Review = -2..+2 group user/memento1
	label-Verified = -1..+1 group user/memento1
	rebase = group user/memento1
	removeReviewer = group user/memento1
	submit = group user/memento1
[access "refs/heads/*"]
	read = group user/memento1
	create = group user/memento1
	push = +force group user/memento1
	pushMerge = group user/memento1
[access "refs/tags/*"]
	push = +force group user/memento1
	pushTag = group user/memento1
	pushSignedTag = group user/memento1
